<!DOCTYPE html>
<html>
  <head>
    <title>Overtime</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="container">
    
    <div class="row">
      <div class="col-md-12">
        <%= render 'shared/nav' %>
        <%= render 'shared/notifications' %>

        <%= yield %>
      </div>
    </div>
  </body>
</html>

